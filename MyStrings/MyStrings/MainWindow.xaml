        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <Button x:Name="button" Content="length of string" HorizontalAlignment="Left" Height="56" Margin="32,29,0,0" VerticalAlignment="Top" Width="102" Click="button_Click"/>
