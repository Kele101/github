        <Button x:Name="button2" Content="IndexOf" HorizontalAlignment="Left" Height="56" Margin="287,29,0,0" VerticalAlignment="Top" Width="88" Click="button2_Click"/>
        <Button x:Name="button3" Content="insert" HorizontalAlignment="Left" Height="56" Margin="415,29,0,0" VerticalAlignment="Top" Width="92" Click="button3_Click"/>
        <Button x:Name="button4" Content="replace" HorizontalAlignment="Left" Height="50" Margin="32,116,0,0" VerticalAlignment="Top" Width="102" Click="button4_Click"/>
        <Button x:Name="button5" Content="delete" HorizontalAlignment="Left" Height="50" Margin="168,116,0,0" VerticalAlignment="Top" Width="98" Click="button5_Click"/>

    </Grid>
